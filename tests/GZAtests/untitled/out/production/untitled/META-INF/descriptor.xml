<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entitiesVTwo.UserEntity">
        <table name="User" schema="" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="email">
                <column name="email"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <basic name="isActivated">
                <column name="isActivated" precision="-1"/>
            </basic>
            <basic name="activationKey">
                <column name="activationKey" nullable="false"/>
            </basic>
            <basic name="salt">
                <column name="salt"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
