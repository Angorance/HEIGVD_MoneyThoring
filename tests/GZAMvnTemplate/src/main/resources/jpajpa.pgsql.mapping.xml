            </id>
            <basic name="name">
                <column name="name" length="50"/>
            </basic>
            <basic name="namebank">
                <column name="namebank" nullable="false" length="50"/>
            </basic>
            <basic name="typeaccount">
                <column name="typeaccount" length="100"/>
            </basic>
            <basic name="amount">
                <column name="amount" precision="17"/>
            </basic>
            <basic name="isdefault">
                <column name="isdefault" precision="1"/>
            </basic>
            <basic name="isvisible">
                <column name="isvisible" precision="1"/>
            </basic>
            <basic name="clientId">
                <column name="client_id" precision="10"/>
            </basic>
            <many-to-one name="clientByClientId" optional="true" target-entity="mt.jpajpa.entities.ClientEntity">
                <join-column name="client_id" referenced-column-name="id"/>
            </many-to-one>
            <one-to-many name="iotransactionsById" mapped-by="bankaccountByBankaccountId"
                         target-entity="mt.jpajpa.entities.IotransactionEntity"/>
