<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dal.entities.derby.BankaccountDeEntity" table="BANKACCOUNT" schema="MONEYTHORING">
        <id name="id">
            <column name="ID" sql-type="integer(10)"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="namebank">
            <column name="NAMEBANK" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="typeaccount">
            <column name="TYPEACCOUNT" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="amount">
            <column name="AMOUNT" sql-type="double(52)" precision="52"/>
        </property>
        <property name="isdefault">
            <column name="ISDEFAULT" sql-type="boolean(1)"/>
        </property>
        <property name="isvisible">
            <column name="ISVISIBLE" sql-type="boolean(1)"/>
        </property>
        <property name="clientId">
            <column name="CLIENT_ID" sql-type="integer(10)"/>
        </property>
        <many-to-one name="clientByClientId" class="dal.entities.derby.ClientDeEntity">
            <column name="CLIENT_ID"/>
        </many-to-one>
        <set name="iotransactionsById" inverse="true">
            <key>
                <column name="BANKACCOUNT_ID"/>
            </key>
            <one-to-many not-found="ignore" class="dal.entities.derby.IotransactionDeEntity"/>
        </set>
    </class>
    <class name="dal.entities.derby.BudgetDeEntity" table="BUDGET" schema="MONEYTHORING">
        <id name="id">
            <column name="ID" sql-type="integer(10)"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="amount">
            <column name="AMOUNT" sql-type="double(52)" precision="52"/>
        </property>
        <property name="isshared">
            <column name="ISSHARED" sql-type="boolean(1)"/>
        </property>
        <property name="isrecurrent">
            <column name="ISRECURRENT" sql-type="boolean(1)"/>
        </property>
        <property name="startingdate">
            <column name="STARTINGDATE" sql-type="date(10)"/>
        </property>
        <property name="endingdate">
            <column name="ENDINGDATE" sql-type="date(10)"/>
        </property>
        <property name="gap">
            <column name="GAP" sql-type="integer(10)" not-null="true"/>
