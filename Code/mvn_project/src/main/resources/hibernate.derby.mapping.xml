        </set>
        <set name="iotransactionsById" inverse="true">
            <key>
                <column name="BUDGET_ID" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="dal.entities.derby.IotransactionDeEntity"/>
        </set>
        <set name="sharedbudgetsById" inverse="true">
            <key>
                <column name="BUDGET_ID"/>
            </key>
            <one-to-many not-found="ignore" class="dal.entities.derby.SharedbudgetDeEntity"/>
        </set>
    </class>
    <class name="dal.entities.derby.CategoriesbudgetDeEntity" table="CATEGORIESBUDGET" schema="MONEYTHORING">
        <composite-id mapped="true" class="dal.entities.derby.CategoriesbudgetDeEntityPK">
            <key-property name="categoryId">
